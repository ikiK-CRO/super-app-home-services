{
  "expo": {
    "name": "Super App Home Services",
    "slug": "super-app-home-services",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#344955"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.superapp.homeservices"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#344955"
      },
      "package": "com.superapp.homeservices"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "0f1ad691-f216-48e8-b77c-66330db54b60"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
